{
  "name": "minesweeper",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://OleksandrYehorov.github.io/minesweeper",
  "repository": {
    "type": "git",
    "url": "https://github.com/OleksandrYehorov/minesweeper"
  },
  "license": "MIT",
  "author": "Oleksandr Yehorov",
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "browserslist-update": "npx browserslist@latest --update-db",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "check-deps-versions": "npx ncu",
    "cy": "cypress open --component",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "dev": "vite --host",
    "lint": "tsc --noEmit && eslint .",
    "preview": "vite preview",
    "test": "cypress run --component"
  },
  "dependencies": {
    "date-fns": "^2.18.0",
    "dseg": "^0.46.0",
    "firebase": "^9.8.3",
    "query-string": "^7.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.2.0",
    "ts-pattern": "^4.0.3",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cypress": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-styled-components-a11y": "^0.0.40",
    "gh-pages": "^4.0.0",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.0"
  }
}
