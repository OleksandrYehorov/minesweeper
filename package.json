{
  "name": "minesweeper",
  "version": "1.0.0",
  "private": true,
  "author": "Oleksandr Yehorov",
  "license": "MIT",
  "homepage": "https://OleksandrYehorov.github.io/minesweeper",
  "repository": {
    "type": "git",
    "url": "https://github.com/OleksandrYehorov/minesweeper"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --watchAll=false",
    "lint": "tsc --noEmit && eslint . --ext .ts,.tsx",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "browserslist-update": "npx browserslist@latest --update-db"
  },
  "dependencies": {
    "date-fns": "^2.18.0",
    "dseg": "^0.46.0",
    "firebase": "^9.8.3",
    "immer": "^9.0.6",
    "query-string": "^6.13.8",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.2.0",
    "ts-pattern": "^2.2.2",
    "zustand": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@vitejs/plugin-react": "^1.3.0",
    "@vitest/ui": "^0.15.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-styled-components-a11y": "^0.0.32",
    "eslint-plugin-testing-library": "^3.10.1",
    "gh-pages": "^3.1.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vitest": "^0.15.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
