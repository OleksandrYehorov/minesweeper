{
  "name": "minesweeper",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://OleksandrYehorov.github.io/minesweeper",
  "repository": {
    "type": "git",
    "url": "https://github.com/OleksandrYehorov/minesweeper"
  },
  "license": "MIT",
  "author": "Oleksandr Yehorov",
  "scripts": {
    "dev": "vite --host",
    "lint": "tsc --noEmit && eslint .",
    "cy": "cypress open --component --browser chrome",
    "test": "cypress run --component",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "npm-check-updates": "npx --yes npm-check-updates",
    "prepreview": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/recipes": "^0.5.0",
    "autoprefixer": "^10.4.14",
    "date-fns": "^2.30.0",
    "firebase": "^10.1.0",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remeda": "^1.24.0",
    "seedrandom": "^3.0.5",
    "ts-pattern": "^5.0.4",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/seedrandom": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitejs/plugin-react": "^4.0.3",
    "cypress": "^12.17.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "prettier": "^3.0.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-pwa": "^0.16.4"
  }
}
