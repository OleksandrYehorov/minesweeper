{
  "name": "minesweeper",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://OleksandrYehorov.github.io/minesweeper",
  "repository": {
    "type": "git",
    "url": "https://github.com/OleksandrYehorov/minesweeper"
  },
  "license": "MIT",
  "author": "Oleksandr Yehorov",
  "scripts": {
    "dev": "vite --host",
    "lint": "tsc --noEmit && eslint .",
    "cy": "cypress open --component",
    "test": "cypress run --component",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "npm-check-updates": "npx --yes npm-check-updates",
    "prepreview": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/recipes": "^0.3.0",
    "date-fns": "^2.29.3",
    "firebase": "^9.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remeda": "^1.3.0",
    "ts-pattern": "^4.0.6",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "@vitejs/plugin-react": "^2.2.0",
    "cypress": "^12.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "prettier": "^2.8.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-pwa": "^0.13.3"
  }
}