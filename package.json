{
  "name": "minesweeper",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://OleksandrYehorov.github.io/minesweeper",
  "repository": {
    "type": "git",
    "url": "https://github.com/OleksandrYehorov/minesweeper"
  },
  "license": "MIT",
  "author": "Oleksandr Yehorov",
  "scripts": {
    "dev": "vite --host",
    "lint": "tsc --noEmit && eslint .",
    "cy": "cypress open --component",
    "test": "cypress run --component",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "npm-check-updates": "npx --yes npm-check-updates",
    "prepreview": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/recipes": "^0.3.0",
    "autoprefixer": "^10.4.13",
    "date-fns": "^2.29.3",
    "firebase": "^9.15.0",
    "immer": "^9.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remeda": "^1.6.0",
    "ts-pattern": "^4.1.3",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "@vitejs/plugin-react": "^3.0.1",
    "cypress": "^12.3.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "prettier": "^2.8.3",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1"
  },
  "overrides": {
    "@vanilla-extract/vite-plugin": {
      "vite": "^4"
    }
  }
}
